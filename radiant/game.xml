<?xml version="1.0"?>
<game
  name="UFO:AI"
  basegame="base"
  surf1="light"
  surf2="slick"
  surf4="warp"
  surf5="blend33"
  surf6="blend66"
  surf7="flow"
  surf8="nodraw"
  surf9="hint"
  surf11="phong"
  surf12="fireaffected"
  surf13="footstep"
  surf26="alphatest"
  cont1="solid"
  cont2="window"
  cont3="ladder"
  cont6="water"
  cont9="level1"
  cont10="level2"
  cont11="level3"
  cont12="level4"
  cont13="level5"
  cont14="level6"
  cont15="level7"
  cont16="level8"
  cont17="actorclip"
  cont18="passable"
  cont19="terrain"
  cont20="lightclip"
  cont25="origin"
  cont26="weaponclip"
  cont28="detail"
  surfaceinspector_enable_value="light"
>
	<defaults>
		<maxWorldCoord value="65536" />
		<minWorldCoord value="-65536" />
	</defaults>

	<filtersystem>
		<filter name="World geometry">
			<filterCriterion type="entityclass" match="worldspawn" action="hide" />
		</filter>
		<filter name="Entities">
			<filterCriterion type="entityclass" match="*" action="hide" />
			<filterCriterion type="entityclass" match="worldspawn" action="show" />
		</filter>
		<filter name="Light">
			<filterCriterion type="entityclass" match="light" action="hide" />
			<filterCriterion type="entityclass" match="light_*" action="hide" />
		</filter>
		<filter name="Models">
			<filterCriterion type="entityclass" match="misc_model" action="hide" />
		</filter>
		<filter name="Particles">
			<filterCriterion type="entityclass" match="misc_particle" action="hide" />
		</filter>
		<filter name="Start positions">
			<filterCriterion type="entityclass" match="info_*_start" action="hide" />
		</filter>
		<filter name="Multiplayer start">
			<filterCriterion type="entityclass" match="info_player_start" action="hide" />
		</filter>
		<filter name="Singleplayer start">
			<filterCriterion type="entityclass" match="info_human_start" action="hide" />
		</filter>
		<filter name="Alien start">
			<filterCriterion type="entityclass" match="info_alien_start" action="hide" />
		</filter>
		<filter name="Civilian start">
			<filterCriterion type="entityclass" match="info_civilian_start" action="hide" />
		</filter>
		<filter name="Common Textures">
			<filterCriterion type="texture" match="textures/tex_common/*" action="hide" />
		</filter>
		<filter name="Actorclip">
			<filterCriterion type="texture" match="textures/tex_common/actorclip" action="hide" />
		</filter>
		<filter name="Lightclip">
			<filterCriterion type="texture" match="textures/tex_common/lightclip" action="hide" />
		</filter>
		<filter name="Weaponclip">
			<filterCriterion type="texture" match="textures/tex_common/weaponclip" action="hide" />
		</filter>
		<filter name="NoDraw">
			<filterCriterion type="texture" match="textures/tex_common/nodraw" action="hide" />
		</filter>
		<filter name="Details">
			<filterCriterion type="contentflags" match="134217728" action="hide" />
		</filter>
	</filtersystem>
</game>
