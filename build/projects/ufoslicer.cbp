<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ufoslicer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="windows">
				<Option platforms="Windows;" />
				<Option output="..\..\ufoslicer" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\" />
				<Option object_output=".objs\ufoslicer" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-ffloat-store" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-mwindows" />
					<Add library="winmm" />
					<Add library="mingw32" />
					<Add library="SDLmain" />
				</Linker>
				<ExtraCommands>
					<Add before='..\..\contrib\scripts\codeblocks_check.bat &quot;$(TARGET_COMPILER_DIR)&quot;' />
				</ExtraCommands>
			</Target>
			<Target title="windows_debug">
				<Option platforms="Windows;" />
				<Option output="..\..\ufoslicer" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\" />
				<Option object_output=".objs\ufoslicer" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option host_application="..\..\ufoslicer" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-pg" />
					<Add option="-DDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
					<Add option="-mwindows" />
					<Add library="winmm" />
					<Add library="mingw32" />
					<Add library="SDLmain" />
				</Linker>
				<ExtraCommands>
					<Add before='..\..\contrib\scripts\codeblocks_check.bat &quot;$(TARGET_COMPILER_DIR)&quot;' />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wcast-align" />
			<Add option="-Winline" />
			<Add option="-Wmissing-declarations" />
			<Add option="-pedantic" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-ggdb" />
			<Add option="-Wcast-qual" />
			<Add option="-Wpointer-arith" />
			<Add option="-Wno-long-long" />
			<Add option="-Wno-sign-compare" />
			<Add option="-Wno-unused-parameter" />
			<Add option="-Wreturn-type" />
			<Add option="-Wwrite-strings" />
			<Add option="-std=c99" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-DWINVER=0x501" />
			<Add option="-DCOMPILE_MAP" />
		</Compiler>
		<Linker>
			<Add library="SDL" />
			<Add library="z" />
			<Add library="jpeg" />
			<Add library="png" />
		</Linker>
		<Unit filename="..\..\src\common\bspslicer.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="..\..\src\common\files.c">
			<Option compilerVar="CC" />
